<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:M64RPFW.ViewModels;assembly=M64RPFW.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        d:DataContext="{x:Type viewModels:SettingsViewModel}"
        x:Class="M64RPFW.Views.Avalonia.Views.SettingsDialog"
        Title="Settings"
        Closed="TopLevel_OnClosed">
    <Window.Resources>
    </Window.Resources>
    <TabControl VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <TabItem Header="General">
            <StackPanel Orientation="Vertical">
                <ComboBox Items="{Binding EmulatorTypes,Mode=OneTime}" SelectedItem="{Binding CoreType,Mode=TwoWay}"/>
                
                <CheckBox Content="Disable Expansion Pak memory"/>
                <CheckBox Content="Randomize interrupt timings"/>
            </StackPanel>
        </TabItem>
        <TabItem Header="Plugins">
            
        </TabItem>
        <TabItem Header="Video">
        </TabItem>
    </TabControl>
</Window>
